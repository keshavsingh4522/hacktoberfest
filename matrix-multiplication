/*hacktoberfest 2021 */
#include <stdio.h>
int main()

{    int N;
  printf("Enter the order of matrix = ");
  scanf("%d",&N);
    int a[N][N],b[N][N],c[N][N],sum=0,d;
     
    printf("enter elements in first matrix of order %dX%d \n",N,N);
    for(int i=0;i<N;i++)
    {
        for(int j=0;j<N;j++)
        {
            scanf("%d",&a[i][j]);
        }
    }
     printf("enter elements in second matrix of order %dX%d \n",N,N);
    for(int i=0;i<N;i++)
    {
        for(int j=0;j<N;j++)
        {
            scanf("%d",&b[i][j]);
        }
    }
    for(int i=0;i<N;i++)
    {sum=0;
        for(int j=0;j<N;j++)
        {for(int k=0;k<N;k++)
        {
           sum+=a[i][k]*b[k][j];
        }
        c[i][j]=sum;
        sum=0;
        }
    }  
    
    printf("\n multiplication matrix result %dX%d matrix \n",N,N);
        for(int i=0;i<N;i++)
        {
            for(int j=0;j<N;j++)
            {
                printf("%d\n",c[i][j]);
            }
        }
    

    return 0;
}
