<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Bingo Dual Player</title>
    <script>
      const BASE_URL = "<%- baseurl %>";
    </script>
    <script src="<%- baseurl %>/libs/jquery-3.5.1.min.js"></script>
    <script
      src="https://cdn.jsdelivr.net/npm/popper.js@1.16.1/dist/umd/popper.min.js"
      integrity="sha384-9/reFTGAW83EW2RDu2S0VKaIzap3H66lZH81PoYlFhbGU+6BZp6G7niu735Sk7lN"
      crossorigin="anonymous"
    ></script>
    <script src="<%- baseurl %>/libs/bs4/bootstrap.min.js"></script>

    <script src="<%- baseurl %>/libs/socket.io/socket.io-3.0.1.min.js"></script>

    <link rel="stylesheet" href="<%- baseurl %>/libs/bs4/bootstrap.min.css" />
    <script
      src="https://kit.fontawesome.com/e8daac6d65.js"
      crossorigin="anonymous"
    ></script>
    <script src="https://cdn.jsdelivr.net/npm/clipboard@2.0.6/dist/clipboard.min.js"></script>
    <script
      src="https://cdnjs.cloudflare.com/ajax/libs/lottie-web/5.7.5/lottie.min.js"
      integrity="sha512-0bCDSnaX8FOD9Mq8WbHcDwshXwCB5V4EP+UBu87WQgga2b7lAsuEbaSmIZjH/XEmNhJuhrPbFHemre5HZwrk9w=="
      crossorigin="anonymous"
    ></script>
  </head>
  <body>
    <%- body %>
    <script>
      const getData = () => {
        let data = window.localStorage.getItem("nBingo_data");
        if (data) {
          data = JSON.parse(data);
          return data;
        } else {
          return null;
        }
      };
      const putData = (data) => {
        data = JSON.stringify(data);
        window.localStorage.setItem("nBingo_data", data);
      };
    </script>
  </body>
</html>
