#include <iostream>
using namespace std;

int main() {
    int t;
    std::cin>>t;
    while(t--)
    {
        int india=0,england=0;
        int r[5];
        
            std::cin>>r[0];
              std::cin>>r[1];
          std::cin>>r[2];
            std::cin>>r[3];
              std::cin>>r[4];
              
        for(int j=0;j<=5;j++)
        {
           if( r[j]==1)
           {
               india++;
           }
           else if(r[j]==2)
           {
               england++;
           }

        }
        if(england>india)
        {
            std::cout<<"ENGLAND"<<endl;
        }
        else if(india>england)
        {
            std::cout<<"INDIA"<<endl;
        }
        else {std::cout<<"DRAW"<<endl;}
    }
    return 0;
}
