#include<algorithm>
class Solution {
public:
    int removeDuplicates(vector<int>& nums) {
       //1 , 1 , 2
    //i=0  j=1
        if(nums.size()==0)
            return 0;
        int i =0;
        for(int j = 1; j< nums.size(); j++)
        {
            if(nums[i]!=nums[j])
                i++;
            nums[i]=nums[j];
        }
        return i+1;
    }
    
};
